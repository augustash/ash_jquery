<layout>
<!--
Default layout, loads most of the pages
-->
    <default>
        <reference name="head">
            <action method="addItem" ifconfig="ash_jquery/jquery/front_enabled"><type>js</type><name>jquery/jquery-1.9.1.min.js</name></action>
            <action method="addItem" ifconfig="ash_jquery/jquery/front_enabled"><type>js</type><name>jquery/jquery-noconflict.js</name></action>
            <action method="addItem" ifconfig="ash_jquery/jquery_ui/front_enabled"><type>js</type><name>jquery/jquery-ui-1.10.0.min.js</name></action>
        </reference>
    </default>
</layout>
